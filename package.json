{
	"name": "svelte-intersection-api-action",
	"version": "0.0.4",
	"description": "A Svelte action to easily integrate the Intersection Observer API",
	"main": "src/index.js",
	"keywords": [
		"svelte",
		"svelte-action",
		"Intersection",
		"Observer",
		"API"
	],
	"author": {
		"name": "anotherempty",
		"url": "https://github.com/anotherempty"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/anotherempty/svelte-intersection-api-action.git"
	},
	"homepage": "https://anotherempty.github.io/svelte-intersection-api-action/",
	"bugs": {
		"url": "https://github.com/anotherempty/svelte-intersection-api-action/issues"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"demo": "vite build",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
